@using TravelTripProje.Models.Siniflar
@model  List<Blog>
<div class="box1-left">
    @foreach(var x in Model)
            {

           
    <div class="bot-img">
        <img src="@x.BlogImage" alt="" />
    </div>
    <div class="bot-desc1">
        <p>@(x.Aciklama.Length > 80 ? x.Aciklama.Substring(0, 80) : x.Aciklama)</p>

    </div>
    <div class="clear"></div>
    }
</div>